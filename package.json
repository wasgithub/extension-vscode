{
    "name": "wasextension",
    "displayName": "wasExtension",
    "description": "HelloWorld",
    "version": "0.0.1",
    "publisher": "WasExtension",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:vscode-docker.configure2"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello",
                "category": "THF"
            },
            {
                "command": "vscode-docker.configure2",
                "title": "Add Docker files to Workspace",
                "description": "Add Dockerfile, docker-compose.yml files",
                "category": "THF"
            }
        ],
        "keybindings": [
            {
                "command": "extension.sayHello",
                "key": "ctrl+f1",
                "mac": "cmd+f1",
                "when": "editorTextFocus"
            }
        ],
        "themes": [
            {
                "label": "wasTheme",
                "uiTheme": "vs-dark",
                "path": "./themes/Monokai.tmTheme"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "package-explorer",
                    "title": "Package Explorer",
                    "icon": "resources/package-explorer.svg"
                }
            ]
        },
        "views": {
            "package-explorer": [
                {
                    "id": "package-dependencies",
                    "name": "Dependencies"
                },
                {
                    "id": "package-outline",
                    "name": "Outline"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode-azureextensionui": "^0.16.6",
        "azure-arm-containerregistry": "^2.3.0",
        "azure-arm-resource": "^2.0.0-preview",
        "azure-arm-website": "^1.0.0-preview",
        "dockerfile-language-server-nodejs": "^0.0.18",
        "dockerode": "^2.5.1",
        "fs-extra": "^6.0.1",
        "glob": "7.1.2",
        "gradle-to-js": "^1.0.1",
        "moment": "^2.19.3",
        "opn": "^5.1.0",
        "pom-parser": "^1.1.1",
        "request-promise": "^4.2.2",
        "vscode-extension-telemetry": "0.0.18",
        "vscode-languageclient": "^4.4.0"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.18",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42",
        "pom-parser": "^1.1.1",
        "gradle-to-js": "^1.0.1",
        "@types/glob": "5.0.35",
        "vscode-azureextensionui": "^0.16.6",
        "@types/adm-zip": "^0.4.31",
        "@types/fs-extra": "^5.0.4",
        "@types/keytar": "^4.0.1",
        "adm-zip": "^0.4.11",
        "azure-storage": "^2.8.1",
        "cross-env": "^5.2.0",
        "gulp": "^3.9.1",
        "mocha": "5.2.0",
        "tslint-microsoft-contrib": "5.0.1",
        "vsce": "^1.37.5"
    }
}
